{
  "name": "juma",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest.json --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --watch",
    "test:all": "jest --config ./test/jest-all.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/sharedKernel/adapters/primary/config/typeorm-migrations.config.ts",
    "migration": "dotenv -e .env -- npm run typeorm migration:run",
    "migration:test": "npm run create:db-test && dotenv -e .env.test -- npm run typeorm migration:run",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:revert": "npm run typeorm migration:revert",
    "fixtures": "dotenv -e .env -- fixtures ./fixtures --config ./dist/config/typeorm-migrations.config.js",
    "fixtures:test": "dotenv -e .env.test -- fixtures ./fixtures --config ./dist/config/typeorm-migrations.config.js",
    "create:db-test": "docker exec -it juma-mysql mysql -u root -pjuma -e \"CREATE SCHEMA IF NOT EXISTS jumatest\"",
    "dev": "concurrently \"npm run start:dev\" \"npm start --prefix frontend\" "
  },
  "dependencies": {
    "@nestjs/bull": "^0.5.5",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "^8.0.4",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "directory-tree": "^3.3.0",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "ioredis": "^5.3.1",
    "mysql2": "^2.3.3",
    "passport": "^0.5.3",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "purify-ts": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.45",
    "uuidv4": "^6.2.13",
    "webtorrent": "^1.8.22"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.8",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4.14.194",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/webtorrent": "^0.109.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "bull": "^4.8.3",
    "concurrently": "^7.2.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "testcontainers": "^11.0.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "4.0.0",
    "typeorm-fixtures-cli": "^1.11.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
