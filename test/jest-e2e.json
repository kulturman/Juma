{
  "setupFiles": ["<rootDir>/test/setEnvVars.ts"],
  "setupFilesAfterEnv": ["<rootDir>/test/test-containers.ts"],
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "modulePaths": [
    "<rootDir>"
  ],
  "testEnvironment": "node",
  "testMatch": ["**/src/**/*.e2e-spec.(ts)"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
